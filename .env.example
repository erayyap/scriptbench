# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL_RUNNER=https://api.openai.com/v1
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0

# Mini SWE configuration
MINI_SWE_MODEL_BASE_URL=https://api.openai.com/v1

# LangChain Configuration
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here

# Logging Configuration
LOG_LEVEL=INFO
DETAILED_LOGS_DIR=logs

# Files Configuration
SCRIPTBENCH_FILES_DIR=files
SCRIPTBENCH_AGENT_FILES_DIR=files_agent

# Execution Configuration
SCRIPT_TIMEOUT=600

# Mini SWE Iterative Backend
MINI_SWE_MINIMUM_ITERATIONS=10

# Test variables
OPENAI_TEST_KEY=your_openai_api_key_here
